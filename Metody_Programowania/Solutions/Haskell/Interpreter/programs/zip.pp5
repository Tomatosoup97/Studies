fun head (x : (int * bool) list) : int * bool =
    match x with
        | [] -> ()
        | a :: as -> a

fun zip (x : int list * bool list) : (int * bool) list =
    match fst x with
    | [] -> []: (int * bool) list
    | a :: as ->  match snd x with
      | [] -> []: (int * bool) list
      | b :: bs -> (a, b) :: zip (as, bs)


